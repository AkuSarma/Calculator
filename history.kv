#:kivy 2.0.0
<History>:
	name: "history"

	BoxLayout:
		orientation: "vertical"
		size: root.width, root.height

		GridLayout:
			size_hint: (1, .1)
			cols: 3

			Button:
				size_hint: (.2, 1)
                font_size: 32
				text: "H"
                disabled: True
				on_press:
					root.manager.current = "history"

			Button:
				font_size: 32
				text: "calculator"
				on_press:
					root.manager.current = "normalcalculator"

			Button:
				font_size: 32
				text: "converter"
				on_press:
					root.manager.current = "unitconverter"

		Button:
			text: "Working on it"
